" Toolbar off
set go-=T
" Menu off
set go-=m
" Left scroll bar off
set go-=L
" Right scroll bar off
set go-=r

" Transparency
set transp=10

" Window size
set lines=50
set columns=100

" Font
"set guifont=Monaco:h16
set guifont="Anonymous Pro":h12

macmenu &File.New\ Tab key=<nop>
macmenu &File.Open\ Tab\.\.\. key=<nop>
